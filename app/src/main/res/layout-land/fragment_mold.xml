                                    app:layout_constraintEnd_toStartOf="@+id/moldvertical45454541"
                                    app:layout_constraintStart_toEndOf="@+id/whitering"
                                    app:layout_constraintTop_toTopOf="@+id/imageView17" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/lightgreenringcheck"
                                    android:layout_width="12dp"
                                    android:layout_height="12dp"
                                    android:elevation="5dp"
                                    android:visibility="invisible"
                                    android:src="@drawable/ic_tick"
                                    app:layout_constraintBottom_toBottomOf="@+id/lightgreenring"
                                    app:layout_constraintEnd_toEndOf="@+id/lightgreenring"
                                    app:layout_constraintStart_toStartOf="@+id/lightgreenring"
                                    app:layout_constraintTop_toTopOf="@+id/lightgreenring" />

                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/whitelinear"
                                    android:layout_width="0dp"
                                    android:layout_height="3dp"
                                    android:layout_marginTop="70dp"
                                    android:background="#ffffff"
                                    android:orientation="horizontal"
                                    android:transitionName=""
                                    app:layout_constraintEnd_toStartOf="@+id/moldvertical4"
                                    app:layout_constraintStart_toStartOf="@+id/moldvertical3"
                                    app:layout_constraintTop_toBottomOf="@+id/greenring">

