                    app:layout_constraintEnd_toStartOf="@+id/moldvertical2"
                    app:layout_constraintStart_toStartOf="@+id/moldvertical12"
                    app:layout_constraintTop_toTopOf="@+id/moldhorizontal1" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/moldbutton"
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:background="@drawable/buttons4"
                    android:fontFamily="@font/cairo_extralight"
                    android:text="Mold"
                    android:gravity="center"
                    android:textAllCaps="false"
                    android:textColor="#2D2C2E"
                    android:textSize="17sp"
                    app:layout_constraintBottom_toTopOf="@+id/moldhorizontal2"
