                                    app:layout_constraintGuide_percent="0.10" />


                                <TextView
                                    android:id="@+id/moldtext"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:text="Choose Your Mold :"
                                    android:textColor="#2D2C2E"
                                    android:textSize="18sp"
                                    android:textStyle="bold"
                                    app:layout_constraintBottom_toBottomOf="@+id/camera"
                                    app:layout_constraintEnd_toStartOf="@+id/moldvertical8"
                                    app:layout_constraintStart_toStartOf="@+id/moldvertical1"
                                    app:layout_constraintTop_toTopOf="@+id/moldhorizontal1" />

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/camera"
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:background="@drawable/buttons5"
                                    android:clickable="true"
                                    android:padding="5dp"
                                    android:src="@drawable/ic_photo_camera_black_24dp"
