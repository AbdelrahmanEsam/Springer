<?xml version="1.0" encoding="utf-8"?>


<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

        <data>
                <variable
                    name="ViewModel"
                    type="com.first.springer.proccessactivites.ProccessViewModel" />
        </data>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/moldfragment"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#F3F3F3"
            tools:context=".proccessactivites.Mold">

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/molddiename"
                    style="@style/TextInputLayout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:boxCornerRadiusBottomEnd="20dp"
                    app:boxCornerRadiusBottomStart="20dp"
                    app:boxCornerRadiusTopEnd="20dp"
                    app:boxCornerRadiusTopStart="20dp"
                    app:boxStrokeColor="#00918E"
                    app:errorEnabled="true"
                    android:imeOptions="actionNext"
                    app:hintTextColor="#00918E"
                    app:layout_constraintEnd_toStartOf="@+id/moldvertical2"
                    app:layout_constraintStart_toStartOf="@+id/moldvertical1"
                    app:layout_constraintTop_toTopOf="@+id/moldhorizontal2">

                        <com.google.android.material.textfield.TextInputEditText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Mold Name"
                            android:textSize="15dp"
                            android:text="@={ViewModel.MoldOrDieName}"
                            android:imeOptions="actionNext"
                            android:singleLine="true"
                            android:textColor="#000000"
                            android:textColorHint="#000000"
                            android:textStyle="normal" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/molddiecode"
                    style="@style/TextInputLayout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:boxCornerRadiusBottomEnd="20dp"
                    app:boxCornerRadiusBottomStart="20dp"
                    app:boxCornerRadiusTopEnd="20dp"
                    app:boxCornerRadiusTopStart="20dp"
                    app:boxStrokeColor="#00918E"
                    app:errorEnabled="true"
                    app:hintTextColor="#00918E"
                    app:layout_constraintBottom_toTopOf="@+id/linearLayoutCompat"
                    app:layout_constraintEnd_toStartOf="@+id/moldvertical2"
                    app:layout_constraintStart_toStartOf="@+id/moldvertical1"
                    app:layout_constraintTop_toBottomOf="@+id/molddiename">

                        <com.google.android.material.textfield.TextInputEditText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Mold Code"
                            android:text="@={ViewModel.MoldOrDieCode}"
                            android:textSize="15dp"
                            android:imeOptions="actionGo"
                            android:inputType="number"
                            android:singleLine="true"
                            android:textColor="#000000"
                            android:textStyle="normal" />
                </com.google.android.material.textfield.TextInputLayout>

                <androidx.appcompat.widget.LinearLayoutCompat
