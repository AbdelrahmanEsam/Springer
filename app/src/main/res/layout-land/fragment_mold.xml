                                    app:layout_constraintTop_toTopOf="@+id/greenring" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/blueringcheck"
                                    android:layout_width="12dp"
                                    android:layout_height="12dp"
                                    android:elevation="5dp"
                                    android:visibility="invisible"
                                    android:src="@drawable/ic_tick"
                                    app:layout_constraintBottom_toBottomOf="@+id/bluering"
                                    app:layout_constraintEnd_toEndOf="@+id/bluering"
                                    app:layout_constraintStart_toStartOf="@+id/bluering"
                                    app:layout_constraintTop_toTopOf="@+id/bluering" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/yellowring"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:elevation="5dp"
                                    android:src="@drawable/yellowring"
                                    app:layout_constraintBottom_toBottomOf="@+id/greenring"
                                    app:layout_constraintEnd_toStartOf="@+id/greenring"
                                    app:layout_constraintStart_toEndOf="@+id/bluering"
                                    app:layout_constraintTop_toTopOf="@+id/greenring" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/yellowringcheck"
                                    android:layout_width="12dp"
                                    android:layout_height="12dp"
                                    android:elevation="5dp"
                                    android:visibility="invisible"
                                    android:src="@drawable/ic_tick"
                                    app:layout_constraintBottom_toBottomOf="@+id/yellowring"
                                    app:layout_constraintEnd_toEndOf="@+id/yellowring"
                                    app:layout_constraintStart_toStartOf="@+id/yellowring"
                                    app:layout_constraintTop_toTopOf="@+id/yellowring" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/greenring"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:src="@drawable/greenring"
                                    android:elevation="5dp"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView17"
                                    app:layout_constraintEnd_toStartOf="@+id/grayring"
                                    app:layout_constraintStart_toEndOf="@+id/yellowring"
                                    app:layout_constraintTop_toTopOf="@+id/imageView17" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/greenringcheck"
                                    android:layout_width="12dp"
                                    android:layout_height="12dp"
                                    android:elevation="5dp"
                                    android:visibility="invisible"
                                    android:src="@drawable/ic_tick"
                                    app:layout_constraintBottom_toBottomOf="@+id/greenring"
                                    app:layout_constraintEnd_toEndOf="@+id/greenring"
