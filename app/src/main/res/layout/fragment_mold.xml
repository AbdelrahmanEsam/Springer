                    android:layout_height="40dp"
                    android:background="@drawable/buttons2"
                    android:fontFamily="@font/cairo_extralight"
                    android:text="Die"
                    android:gravity="center"
                    android:textAllCaps="false"
                    android:textColor="#01928F"
                    android:textSize="17sp"
                    app:layout_constraintBottom_toTopOf="@+id/moldhorizontal2"
                    app:layout_constraintEnd_toStartOf="@+id/moldvertical4"
                    app:layout_constraintStart_toStartOf="@+id/moldvertical11"
                    app:layout_constraintTop_toTopOf="@+id/moldhorizontal6" />


                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/next"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/buttons3"
                    android:fontFamily="@font/cairo_extralight"
                    android:text="Next"
                    android:gravity="center"
                    android:textAllCaps="false"
                    android:textColor="#FFFFFF"
                    android:textSize="20sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/moldvertical1212215"
                    app:layout_constraintStart_toStartOf="@+id/moldvertical153"
                    app:layout_constraintTop_toTopOf="@+id/moldhorizontal5" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Color"
                    android:textColor="#2D2C2E"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/imageView6"
                    app:layout_constraintStart_toStartOf="@+id/moldvertical1"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayoutCompat" />


                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/orangering"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/orangering"
                    android:elevation="5dp"
                    app:layout_constraintBottom_toTopOf="@+id/moldhorizontal4"
                    app:layout_constraintEnd_toStartOf="@+id/moldvertical1"
                    app:layout_constraintStart_toStartOf="@+id/moldvertical1"
                    app:layout_constraintTop_toTopOf="@+id/imageView6" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/orangeringcheck"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:elevation="5dp"
                    android:visibility="invisible"
                    android:src="@drawable/ic_tick"
                    app:layout_constraintBottom_toBottomOf="@+id/orangering"
                    app:layout_constraintEnd_toEndOf="@+id/orangering"
                    app:layout_constraintStart_toStartOf="@+id/orangering"
                    app:layout_constraintTop_toTopOf="@+id/orangering" />


                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/redring"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/redring"
                    android:elevation="5dp"
                    app:layout_constraintBottom_toTopOf="@+id/moldhorizontal4"
                    app:layout_constraintEnd_toStartOf="@+id/bluering"
                    app:layout_constraintStart_toEndOf="@+id/orangering"
                    app:layout_constraintTop_toTopOf="@+id/imageView6" />

                <androidx.appcompat.widget.AppCompatImageView
