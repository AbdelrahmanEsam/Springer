                                    android:src="@drawable/ic_tick"
                                    app:layout_constraintBottom_toBottomOf="@+id/grayring"
                                    app:layout_constraintEnd_toEndOf="@+id/grayring"
                                    app:layout_constraintStart_toStartOf="@+id/grayring"
                                    app:layout_constraintTop_toTopOf="@+id/grayring" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/whitering"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:src="@drawable/whitering"
                                    android:elevation="5dp"
                                    app:layout_constraintBottom_toBottomOf="@+id/imageView17"
                                    app:layout_constraintEnd_toStartOf="@+id/lightgreenring"
                                    app:layout_constraintStart_toEndOf="@+id/grayring"
                                    app:layout_constraintTop_toTopOf="@+id/imageView17" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/whiteringcheck"
                                    android:layout_width="12dp"
                                    android:layout_height="12dp"
                                    android:elevation="5dp"
                                    android:visibility="invisible"
                                    android:src="@drawable/ic_tick"
                                    app:layout_constraintBottom_toBottomOf="@+id/whitering"
                                    app:layout_constraintEnd_toEndOf="@+id/whitering"
                                    app:layout_constraintStart_toStartOf="@+id/whitering"
                                    app:layout_constraintTop_toTopOf="@+id/whitering" />

