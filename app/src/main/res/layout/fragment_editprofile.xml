                    app:layout_constraintGuide_end="260dp"
                    app:layout_constraintGuide_percent="0.90" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/save"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/buttons3"
                    android:fontFamily="@font/cairo_extralight"
                    android:text="Save"
                    android:gravity="center"
                    android:textAllCaps="false"
                    android:textColor="#FFFFFF"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal3"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical4"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical3"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal14" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Personal Information"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:textColor="#000000"
                    android:textSize="16sp"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal4"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical9"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal2545" />

                <TextView
                    android:id="@+id/namehint"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="Name"
                    android:textAlignment="center"
                    android:textColor="#707070"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal5"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical5"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal4" />

                <ImageView
                    android:id="@+id/namehintimage"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:src="@drawable/person"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal5"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical1"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal4" />

                <ImageView
                    android:id="@+id/emailhintimage"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:src="@drawable/mail"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal7"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical1"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal6" />


                gender

                <ImageView
                    android:id="@+id/genderedithintimage"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:src="@drawable/ic_gender"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal11"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical1"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal10" />

                <ImageView
                    android:id="@+id/passedithintimage"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:src="@drawable/pass"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal13"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical1"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal12" />


                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/yournameedit"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:boxCornerRadiusBottomEnd="20dp"
                    app:boxCornerRadiusBottomStart="20dp"
                    app:boxCornerRadiusTopEnd="20dp"
                    app:boxCornerRadiusTopStart="20dp"
                    app:boxStrokeColor="#00918E"
                    app:hintEnabled="false"
                    app:hintTextColor="#00918E"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal6"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical9"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical1"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal5">

                    <com.google.android.material.textfield.TextInputEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="1"
                        android:gravity="left"
                        android:inputType="textPersonName"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:padding="15dp"
                        android:text="@={ViewModel.EditName}"
                        android:textSize="15sp" />
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:id="@+id/emailhint"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="E-mail"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textColor="#707070"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal7"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical5"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal6" />

                <TextView
                    android:id="@+id/genderhint"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Gender"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textColor="#707070"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal11"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical6"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal10" />

                <TextView
                    android:id="@+id/maletext"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Male"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal12"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical6"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical3"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal11" />


                <TextView
                    android:id="@+id/femaletext"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Female"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal12"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical9"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical8"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal11" />

                <TextView
                    android:id="@+id/agehint"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Age"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textColor="#707070"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal9"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical5"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal8" />

                <TextView
                    android:id="@+id/passhint"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Password"
                    android:gravity="center"
                    android:textAlignment="center"
                    android:textColor="#707070"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal13"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical6"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal12" />

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:src="@drawable/ic_age"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal9"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical1"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal8" />

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/youremailedit"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:boxCornerRadiusBottomEnd="20dp"
                    app:boxCornerRadiusBottomStart="20dp"
                    app:boxCornerRadiusTopEnd="20dp"
                    app:boxCornerRadiusTopStart="20dp"
                    app:boxStrokeColor="#00918E"
                    app:hintEnabled="false"
                    app:hintTextColor="#00918E"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal8"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical9"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical1"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal7">

                    <com.google.android.material.textfield.TextInputEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="1"
                        android:inputType="textEmailAddress"
                        android:textColor="#000000"
                        android:gravity="left"
                        android:text="@={ViewModel.EditEmail}"
                        android:textColorHint="#000000"
                        android:textSize="15sp"
                        android:padding="15dp"
                        android:textStyle="normal" />
                </com.google.android.material.textfield.TextInputLayout>


                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/youragedit"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:boxCornerRadiusBottomEnd="20dp"
                    app:boxCornerRadiusBottomStart="20dp"
                    app:boxCornerRadiusTopEnd="20dp"
                    app:boxCornerRadiusTopStart="20dp"
                    app:boxStrokeColor="#00918E"
                    app:hintTextColor="#00918E"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal10"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical9"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical1"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal9">

                    <com.google.android.material.textfield.TextInputEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="numberDecimal"
                        android:maxLines="1"
                        android:textColor="#000000"
                        android:gravity="left"
                        android:text="@={ViewModel.EditAge}"
                        android:textColorHint="#000000"
                        android:textSize="15sp"
                        android:textStyle="normal"
                        android:padding="15dp"
                        />

                </com.google.android.material.textfield.TextInputLayout>


                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/yourpassdit"
                    style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:boxCornerRadiusBottomEnd="20dp"
                    app:boxCornerRadiusBottomStart="20dp"
                    app:boxCornerRadiusTopEnd="20dp"
                    app:boxCornerRadiusTopStart="20dp"
                    app:boxStrokeColor="#00918E"
                    app:hintTextColor="#00918E"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal14"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical9"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical1"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal13"
                    app:passwordToggleEnabled="true"
                    app:passwordToggleTint="#A0A0A0">

                    <com.google.android.material.textfield.TextInputEditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textPassword"
                        android:maxLines="1"
                        android:text="@={ViewModel.EditPassword}"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textSize="15sp"
                        android:gravity="left"
                        android:padding="15dp"
                        android:textStyle="normal" />
                </com.google.android.material.textfield.TextInputLayout>

                <ImageView
                    android:id="@+id/maleselect"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/bordercircle"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal12"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical3"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical2"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal11" />

                <ImageView
                    android:id="@+id/femaleselect"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/bordercircle"
                    app:layout_constraintBottom_toTopOf="@+id/profilecardhorizontal12"
                    app:layout_constraintEnd_toStartOf="@+id/femaletext"
                    app:layout_constraintStart_toStartOf="@+id/profilecardvertical7"
                    app:layout_constraintTop_toTopOf="@+id/profilecardhorizontal11" />

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/imageView12"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:clickable="true"
                    android:elevation="5dp"
                    android:src="#ffffff"
                    app:layout_constraintBottom_toBottomOf="@+id/selectimage"
                    app:layout_constraintEnd_toStartOf="@+id/profilecardvertical8"
                    app:layout_constraintTop_toBottomOf="@+id/selectimage" />

                <ImageView
                    android:id="@+id/imageView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:elevation="6dp"
                    android:src="@drawable/ic_camera_alt_black_24dp"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView12"
                    app:layout_constraintEnd_toEndOf="@+id/imageView12"
                    app:layout_constraintStart_toStartOf="@+id/imageView12"
                    app:layout_constraintTop_toTopOf="@+id/imageView12" />


            </androidx.constraintlayout.widget.ConstraintLayout>


        </com.google.android.material.card.MaterialCardView>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/profilehorizontal1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="20dp"
            app:layout_constraintGuide_percent="0.10" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/profilehorizontal2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
