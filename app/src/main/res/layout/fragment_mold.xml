                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/grayring"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/gray"
                    android:elevation="5dp"
                    app:layout_constraintBottom_toTopOf="@+id/moldhorizontal4"
                    app:layout_constraintEnd_toStartOf="@+id/whitering"
                    app:layout_constraintStart_toEndOf="@+id/greenring"
                    app:layout_constraintTop_toTopOf="@+id/imageView6" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/grayringcheck"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:elevation="5dp"
                    android:visibility="invisible"
                    android:src="@drawable/ic_tick"
                    app:layout_constraintBottom_toBottomOf="@+id/grayring"
                    app:layout_constraintEnd_toEndOf="@+id/grayring"
                    app:layout_constraintStart_toStartOf="@+id/grayring"
                    app:layout_constraintTop_toTopOf="@+id/grayring" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/whitering"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
