                                    android:id="@+id/imageView17"
                                    android:layout_width="0dp"
                                    android:layout_height="30dp"
                                    android:layout_marginTop="40dp"
                                    app:layout_constraintEnd_toStartOf="@+id/moldvertical45454541"
                                    app:layout_constraintStart_toStartOf="@+id/moldvertical154545"
