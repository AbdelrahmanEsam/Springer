                    android:layout_height="1dp"
                    android:background="#707070"
                    app:layout_constraintBottom_toTopOf="@+id/moldhorizontal3"
                    app:layout_constraintEnd_toStartOf="@+id/moldvertical2"
                    app:layout_constraintHorizontal_bias="0.36"
                    app:layout_constraintStart_toStartOf="@+id/moldvertical1"
                    app:layout_constraintTop_toBottomOf="@+id/molddiecode" />

                <androidx.constraintlayout.widget.Guideline
