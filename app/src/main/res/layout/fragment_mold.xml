                    app:layout_constraintBottom_toTopOf="@+id/moldhorizontal6"
                    app:layout_constraintEnd_toStartOf="@+id/moldvertical11"
                    app:layout_constraintStart_toStartOf="@+id/moldvertical1"
                    app:layout_constraintTop_toTopOf="@+id/moldhorizontal1" />

                <de.hdodenhof.circleimageview.CircleImageView
