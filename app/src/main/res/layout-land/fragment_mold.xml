                                    android:layout_height="20dp"
                                    android:elevation="5dp"
                                    android:src="@drawable/orangering"
                                    app:layout_constraintBottom_toBottomOf="@+id/greenring"
                                    app:layout_constraintEnd_toStartOf="@+id/redring"
                                    app:layout_constraintStart_toStartOf="@+id/moldvertical154545"
                                    app:layout_constraintTop_toTopOf="@+id/greenring" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/orangeringcheck"
                                    android:layout_width="12dp"
                                    android:layout_height="12dp"
                                    android:elevation="5dp"
                                    android:visibility="invisible"
                                    android:src="@drawable/ic_tick"
                                    app:layout_constraintBottom_toBottomOf="@+id/orangering"
                                    app:layout_constraintEnd_toEndOf="@+id/orangering"
                                    app:layout_constraintStart_toStartOf="@+id/orangering"
                                    app:layout_constraintTop_toTopOf="@+id/orangering" />


                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/redring"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:elevation="5dp"
                                    android:src="@drawable/redring"
                                    app:layout_constraintBottom_toBottomOf="@+id/greenring"
                                    app:layout_constraintEnd_toStartOf="@+id/bluering"
                                    app:layout_constraintStart_toEndOf="@+id/orangering"
                                    app:layout_constraintTop_toTopOf="@+id/greenring" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/redringcheck"
                                    android:layout_width="12dp"
                                    android:layout_height="12dp"
                                    android:elevation="5dp"
                                    android:visibility="invisible"
                                    android:src="@drawable/ic_tick"
                                    app:layout_constraintBottom_toBottomOf="@+id/redring"
                                    app:layout_constraintEnd_toEndOf="@+id/redring"
                                    app:layout_constraintStart_toStartOf="@+id/redring"
                                    app:layout_constraintTop_toTopOf="@+id/redring" />

                                <androidx.appcompat.widget.AppCompatImageView
                                    android:id="@+id/bluering"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:elevation="5dp"
                                    android:src="@drawable/bluering"
                                    app:layout_constraintBottom_toBottomOf="@+id/greenring"
                                    app:layout_constraintEnd_toStartOf="@+id/yellowring"
